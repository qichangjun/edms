{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "paths": {
      "@commonService/*":["app/services/*"],
      "@coreModule/*" : ["app/core/*"],
      "@fileBaseModule/*" : ["app/home/fileBase/*"],
      "@userModule/*" : ["app/home/userManage/user/*"],
      "@groupModule/*" : ["app/home/userManage/group/*"],
      "@roleModule/*" : ["app/home/userManage/role/*"]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  } 
}
